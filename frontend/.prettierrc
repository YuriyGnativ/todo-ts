{
  "trailingComma": "none",
  "arrowParens": "avoid",
  "singleQuote": true,
  "useTabs": false,
  "tabWidth": 2,
  "semi": true,
  "importOrder": [
    "react",
    "^(?!react)\\w+$",
    "<THIRD_PARTY_MODULES>",
    "^api(.*)$",
    "^auth(.*)$",
    "^tailwind(.*)$",
    "^app(.*)$",
    "^pages(.*)$",
    "^layout(.*)$",
    "^components(.*)$",
    "^panels(.*)$",
    "^@types(.*)$",
    "^utils(.*)$",
    "^assets(.*)$",
    "^icons(.*)$",
    "^property-icons(.*)$",
    "^@fontsource(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true
}
